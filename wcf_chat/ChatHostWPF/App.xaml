<Application x:Class="ChatHostWPF.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="clr-namespace:ChatHostWPF"
			 StartupUri="MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<!-- Order is important -->
				<ResourceDictionary Source="pack://application:,,,/WPF.Core;component/Styles/Colors.xaml" />
				<ResourceDictionary Source="pack://application:,,,/WPF.Core;component/Styles/Fonts.xaml" />
				<ResourceDictionary Source="pack://application:,,,/WPF.Core;component/Styles/Buttons.xaml" />
				<ResourceDictionary Source="pack://application:,,,/WPF.Core;component/Styles/Texts.xaml" />
				<ResourceDictionary Source="pack://application:,,,/WPF.Core;component/Styles/Scroll.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
